@{
    ViewBag.Title = "Đây là trang chủ";
}
@using LearLogin_DayNhauHoc.Models
@model  List<Type>
<ul>
    @{
        for (int i = 0; i < Model.Count; i++)
        {
            <li>
                <h1>Controller : @Model[i].Name</h1>
                @{
                    var lstAction = ReFlecttionController.GetActions(@Model[i]);
                    for (int j = 0; j < lstAction.Count; j++)
                    {
                        <a href="~/Home/@lstAction[j]">--> @lstAction[j]</a>
                        <br />
                    }
                }
            </li>
        }
    }
</ul>